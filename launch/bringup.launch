<launch>
  <include file="$(find indoor_localization)/launch/bringup.launch"/>
  <node pkg="laser_assembler" type="laser_scan_assembler" name="laser_assembler">
    <param name="~fixed_frame" value="odom"/>
  </node>

  <include file="$(find raspicam_node)/launch/camerav2_1280x960.launch">
    <param name="~camera_info_url" value="package://crawler/camera_info/skycam.yaml"/>
  </include>
  <node pkg="image_proc" type="image_proc" name="skycam" ns="/raspicam_node" >
  </node>
</launch>
